<?xml version="1.0"?>
<package >
  <metadata>
    <id>{package_name}</id>
    <version>{version}</version>
    <authors>FluentDataAccess</authors>
    <owners>gissehel</owners>
    <projectUrl>https://github.com/gissehel/FluentDataAccess</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/gissehel/FluentDataAccess/master/res/FluentDataAccess-32.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A simple C# Fluent API to start SQL queries (Support only SQLite for now).{description_add}</description>
    <releaseNotes></releaseNotes>
    <copyright>MIT</copyright>
    <tags>FluentDataAccess DataAccess Fluent SQLite</tags>
    <dependencies>
        <dependency id="System.Data.SQLite" version="1.0.108" />
        <dependency id="System.Data.SQLite.Core" version="1.0.108" />
        <dependency id="System.Data.SQLite.Linq" version="1.0.108" />
    </dependencies>
  </metadata>
  <files>
    <file src="{package_path}\bin\Release\netstandard2.0\{package_name}.dll" target="lib\netstandard2.0\{package_name}.dll" />
    <file src="{package_path}\bin\Release\net35\{package_name}.dll" target="lib\net35\{package_name}.dll" />
    <file src="{package_path}\bin\Release\net35\System.Data.SQLite.dll" target="lib\net35\System.Data.SQLite.dll" />
    <file src="{package_path}\bin\Release\net35\x86\SQLite.Interop.dll" target="lib\net35\x86\SQLite.Interop.dll" />
    <file src="{package_path}\bin\Release\net35\x64\SQLite.Interop.dll" target="lib\net35\x64\SQLite.Interop.dll" />
  </files> 
</package>
